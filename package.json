{
  "name": "kindred-paths",
  "version": "0.0.1",
  "description": "A tool for managing the cards in Kindred Paths, the custom Magic the Gathering set by Simon Karman.",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm --prefix shared install && npm --prefix client install && npm --prefix server install",
    "dev": "npm-run-all --parallel dev:*",
    "dev:shared": "npm run --prefix shared dev",
    "dev:server": "npm run --prefix server dev",
    "dev:client": "npm run --prefix client dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonkarman/kindred-paths.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/simonkarman/kindred-paths/issues"
  },
  "homepage": "https://github.com/simonkarman/kindred-paths#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19"
  }
}
